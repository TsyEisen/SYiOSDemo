#import "Banner.h"

@implementation Banner

- (void)setType:(BannerType)type {
    _type = type;
    switch (type) {
        case BannerTypeHX:
            self.title = @"户型图";
            break;
        case BannerTypeXQ:
            self.title = @"小区图";
            break;
    }
}

@end
